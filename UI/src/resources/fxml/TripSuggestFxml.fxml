<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane minHeight="220.0" minWidth="100.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TripSuggestController">
   <content>
      <VBox id="vbox" prefHeight="343.0" prefWidth="157.0" spacing="5.0" styleClass="vbox" stylesheets="@../../../../UI/src/CSS/vbox.css">
         <children>
            <TextArea accessibleRole="TEXT" editable="false" prefHeight="33.0" prefWidth="128.0" styleClass="textArea" stylesheets="@../../../../UI/src/CSS/textArea.css" text="Trip Request ">
               <font>
                  <Font name="System Font" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </padding>
               <effect>
                  <DropShadow height="24.04" radius="11.52" spread="0.08" width="24.04" />
               </effect>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </TextArea>

            <Button alignment="TOP_RIGHT" mnemonicParsing="false" prefHeight="32.0" prefWidth="61.0" text="Add"
                    textAlignment="JUSTIFY" textOverrun="CLIP" translateX="25.0" translateY="200.0" onAction="#addTripSuggestButtonActionListener">
               <font>
                  <Font size="12.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </VBox.margin>
               <padding>
                  <Insets right="22.0" top="5.0" />
               </padding>
            </Button>
         </children>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </VBox>
   </content>
</ScrollPane>
