<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.ToolBar?>

<ScrollPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MatchingController">
   <content>
      <ToolBar prefHeight="90.0" prefWidth="900.0">
        <items>
          <Button fx:id="matchingButton" mnemonicParsing="false" onAction="#matchingButtonActionListener" prefHeight="28.0" prefWidth="140.0" text="Matching Action:" translateX="30.0" translateY="-25.0" />
            <Accordion fx:id="matchingAccordion" prefHeight="68.0" prefWidth="94.0" translateX="100.0">
              <panes>
                <TitledPane animated="false" text="Current Matching trips" />
              </panes>
            </Accordion>
        </items>
      </ToolBar>
   </content>
</ScrollPane>
