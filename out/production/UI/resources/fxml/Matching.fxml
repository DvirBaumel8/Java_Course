<ScrollPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MatchingController">
<content>
    <ToolBar prefHeight="160.0" prefWidth="995.0" styleClass="matching" stylesheets="@../../CSS/matching.css">
        <items>
            <Button fx:id="matchingButton" mnemonicParsing="false" onAction="#matchingButtonActionListener" prefHeight="70.0" prefWidth="300.0" text="Go Match" translateX="100.0" translateY="-15.0">
                <font>
                    <Font size="16.0" />
                </font></Button>
            <Label prefHeight="29.0" prefWidth="177.0" text="Current Suggests Trips:" translateX="340.0" translateY="-60.0" underline="true">
               <font>
                  <Font size="15.0" />
               </font></Label>
            <ScrollPane minHeight="120" minWidth="320.0" prefHeight="120" prefWidth="320.0" styleClass="currentTrips" stylesheets="@../../CSS/currentTrips.css" translateX="345.0">
            <VBox fx:id="currSuggestTripsVBox" minWidth="310.0" prefHeight="16.0" prefWidth="310.0" translateX="2">
               <children>
               </children>
            </VBox>
            </ScrollPane>
        </items>
    </ToolBar>
</content>
</ScrollPane>
